<h1>Services#edit <%= @service.menu %> </h1>
<p>Find me in app/views/services/edit.html.erb</p>

<p><%= @service.menu_name %></p>
<p><%= @service.menu %></p>
<p><%= @service.price %></p>
<p><%= @service.max_quantity %></p>
<p><%= @service.picture %></p>
<p><%= @service.category %></p>
<p><%= @service.date %></p>
<p><%= @service.shift %></p>

<%= simple_form_for @service do |f| %>
  <%= f.input :menu_name %>
  <%= f.input :menu %>
  <%= f.input :price %>
  <%= f.input :max_quantity %>
  <%= f.input :category %>
  <%= f.input :date %>
  <%= f.input :shift %>
  <%= f.input :picture %>
  <%= f.input :user_id %>
  <!-- add cloudinary public api "string" -->
  <%= f.button :submit %>
<% end %>
